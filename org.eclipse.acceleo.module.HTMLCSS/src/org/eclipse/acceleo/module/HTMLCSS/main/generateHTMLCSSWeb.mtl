  [/for]
[/template]

[template public generateResultadoEncuesta(e : Encuesta)]
  [for (acceso : Rol | e.usuario)]
    [file ('/HTML/'.concat(acceso.Nombre).concat('/P_Resultado_Encuesta_').concat(e.Nombre).concat('.html'), false, 'UTF-8')]

<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <title>Resultado de la encuesta [e.Nombre/]</title>
  <link rel="stylesheet" href="../styles.css">
</head>
<body>
  <!-- Header Principal -->
  <div class="header">
    [for (ent : Entidad | e.entidad)]
      <h1>[Aplicacion.allInstances()->any(rol | rol.paginapantallas->includes(e)).Nombre/]</h1>
    [/for]
  </div>

  <!-- Subheader -->
  <div class="subheader">
    <h2>Resultado de la encuesta [e.Nombre/]</h2>
  </div>

  <div class="container">
    <div class="main-content">
      <div class="content">
        <h3>Preguntas respondidas:</h3>
        <ol>
          [for (preg : Pregunta | e.preguntasE)]
            <li>
              <p><strong>Pregunta [e.preguntasE->indexOf(preg) + 1/]:</strong> [preg.Texto/]</p>

              [if (preg.Tipo = TipoPregunta::OpcionMultiple)]
                [for (resp : Respuesta | preg.respuestas)]
                  <label>
                    <input type="radio" disabled> [resp.Texto/]
                  </label><br>
                [/for]
              [/if]

              [if (preg.Tipo = TipoPregunta::V_F)]
                <label><input type="radio" disabled> Verdadero</label><br>
                <label><input type="radio" disabled> Falso</label><br>
              [/if]

              [if (preg.Tipo = TipoPregunta::Corta)]
                <input type="text" disabled placeholder="Respuesta del usuario"><br>
              [/if]
            </li>
          [/for]
        </ol>
      </div>
    </div>

    <div class="sidebar social">
      <h3>Redes Sociales:</h3>
      <div class="twitter-timeline-container">
        <a class="twitter-timeline" 
           data-width="100%" 
           data-height="400"
           href="https://twitter.com/infouex?ref_src=twsrc%5Etfw">Tweets by infouex</a>
      </div>
      <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>

      [for (rs : RedSocial | e.redsocial)]
        <p>- <a href="https://www.[rs.Nombre.toLowerCase()/].com" target="_blank" class="social-link">
            [rs.Nombre/]</a></p>
      [/for]
    </div>
  </div>

  <div class="button-container">
    <button onclick="window.history.back()" class="button">Volver</button>
  </div>

  <div class="footer">
    <p>Aplicacion generada automaticamente - Todos los derechos reservados</p>
    <p>Sergio Terrazas Lobato y Javier Costillo Palacios</p>
  </div>
</body>
</html>

    [/file]
  [/for]
[/template]
