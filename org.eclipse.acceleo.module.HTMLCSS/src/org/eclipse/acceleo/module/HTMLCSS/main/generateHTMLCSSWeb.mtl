<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <title>Encuesta de [c.Nombre/]</title>
  <link rel="stylesheet" href="../styles.css">
</head>
<body>

<!-- Header Principal -->
<div class="header">
  [for (ent : Entidad | c.entidad)]
    <h1>[Aplicacion.allInstances()->any(rol | rol.paginapantallas->includes(c)).Nombre/]</h1>
  [/for]
</div>

<!-- Subheader -->
<div class="subheader">
  <h2>Encuesta de [c.Nombre/]</h2>
</div>

  <div class="container">
    <div class="main-content">
      <div class="content">
        <h3>Descripcion de la Encuesta:</h3>
        <p>[c.Descripcion/]</p>
		<div class="atributos-derivados">
		  <h2>Resumen de preguntas</h2>
		  <ul>
		    <li><strong>Numero de preguntas Verdadero/Falso:</strong> <span>[c.numeroPregVF/]</span></li>
		    <li><strong>Numero de preguntas de Seleccion multiple:</strong> <span>[c.numeroPregSelect/]</span></li>
		  </ul>
		</div>

        <!-- Formulario para responder la encuesta -->
        <form action="resultado_encuesta_[c.Nombre.toLowerCase()/].html" method="post">
          
          <!-- Iteramos sobre las preguntas de la encuesta -->
          [for (preg : Pregunta | c.preguntasE)]
            <div class="pregunta">
              <p><strong>Pregunta [c.preguntasE->indexOf(preg)/]:</strong> [preg.Texto/]</p>

              <!-- Si la pregunta es de opción múltiple -->
              [if (preg.Tipo = TipoPregunta::OpcionMultiple)]
                [for (resp : Respuesta | preg.respuestas)]
                  <label>
                    <input type="radio" name="preg_[c.preguntasE->indexOf(preg) + 1/]" value="[resp.Texto/]"> [resp.Texto/]
                  </label><br>
                [/for]
              [/if]

              <!-- Si la pregunta es de verdadero o falso -->
              [if (preg.Tipo = TipoPregunta::V_F)]
                <label><input type="radio" name="preg_[c.preguntasE->indexOf(preg) + 1/]" value="Verdadero"> Verdadero</label><br>
                <label><input type="radio" name="preg_[c.preguntasE->indexOf(preg) + 1/]" value="Falso"> Falso</label><br>
              [/if]

              <!-- Si la pregunta es de respuesta corta -->
              [if (preg.Tipo = TipoPregunta::Corta)]
                <input type="text" name="preg_[c.preguntasE->indexOf(preg) + 1/]" placeholder="Escribe tu respuesta..."><br>
              [/if]
            </div>
          [/for]

          <div class="actions" style="margin-top: 30px;">
            <button type="button" onclick="enviarRespuestas()">Enviar respuestas</button>
          </div>
			<div class="button-container">
			  <a href="P_Index_[acceso.Nombre/].html" class="button">Volver</a>
			</div>
<script>
            function enviarRespuestas() {
              window.location.href = "P_Resultado_Encuesta_" + "[c.Nombre/]" + ".html";
            }
          </script>
        </form>
      </div>
    </div>

   <div class="sidebar social">
            <h3>Redes Sociales:</h3>
            <!-- Mostrar el timeline de Twitter dentro del contenedor -->
            <div class="twitter-timeline-container">
              <a class="twitter-timeline" 
                 data-width="100%" 
                 data-height="400"
                 href="https://twitter.com/infouex?ref_src=twsrc%5Etfw">Tweets by infouex</a>
            </div>
          
            <!-- Cargar el script de Twitter de manera asíncrona -->
            <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
          
            <!-- Mostrar las demás redes sociales como enlaces -->
            [for (rs : RedSocial | c.redsocial)]
              <p>- <a href="https://www.[rs.Nombre.toLowerCase()/].com" target="_blank" class="social-link">
                  [rs.Nombre/]</a></p>
            [/for]
          </div>
  </div>
  <div class="footer">
    <p>Aplicacion generada automaticamente - Todos los derechos reservados</p>
	<p>Sergio Terrazas Lobato y Javier Costillo Palacios</p>
  </div>

</body>
