[template public generatePagina(p : PaginaPantalla)]

  [if (p.oclIsKindOf(IndiceEntidad))]
    [let ie : IndiceEntidad = p.oclAsType(IndiceEntidad)]
      [generateIndiceEntidad(ie)/]
    [/let]
  [/if]

  [if (p.oclIsKindOf(IndiceExtendido))]
    [let iex : IndiceExtendido = p.oclAsType(IndiceExtendido)]
      [generateIndiceExtendido(iex)/]
    [/let]
  [/if]

  [if (p.oclIsKindOf(IndiceDetalle))]
    [let id : IndiceDetalle = p.oclAsType(IndiceDetalle)]
      [generateIndiceDetalle(id)/]
    [/let]
  [/if]

  [if (p.oclIsKindOf(DetallesInstancia))]
    [let di : DetallesInstancia = p.oclAsType(DetallesInstancia)]
      [generateDetalle(di)/]
    [/let]
  [/if]

  [if (p.oclIsKindOf(CrearInstancia))]
    [let ci : CrearInstancia = p.oclAsType(CrearInstancia)]
      [generateCreacion(ci)/]
    [/let]
  [/if]

  [if (p.oclIsKindOf(BorradoInstancia))]
    [let bi : BorradoInstancia = p.oclAsType(BorradoInstancia)]
      [generateBorrado(bi)/]
    [/let]
  [/if]

  [if (p.oclIsKindOf(CRUD))]
	 [let crud : CRUD = p.oclAsType(CRUD)]
	    [generateCRUD(crud)/]
	  [/let]
	[/if]


  [if (p.oclIsKindOf(Cuestionario))]
    [let c : Cuestionario = p.oclAsType(Cuestionario)]
      [generateCuestionario(c)/]
      [generateResultadoCuestionario(c)/]
    [/let]
  [/if]

  [if (p.oclIsKindOf(Encuesta))]
    [let e : Encuesta = p.oclAsType(Encuesta)]
      [generateEncuesta(e)/]
      [generateResultadoEncuesta(e)/]
    [/let]
  [/if]

  [if (p.oclIsKindOf(ActualizacionInstancia))]
    [let a : ActualizacionInstancia = p.oclAsType(ActualizacionInstancia)]
      [generateActualizacion(a)/]
    [/let]
  [/if]

  [if (p.oclIsKindOf(Normal))]
    [let n : Normal = p.oclAsType(Normal)]
      [generateContenidoNormal(n)/]
    [/let]
  [/if]

[/template]

[template public generateCreacion(c : CrearInstancia)]
