      Descripcion="Cuestionario de ámbito informático">
    <preguntasC
        Tipo="Corta"
        Texto="¿Qué es un sistema operativo?">
      <respuestas
          Texto="Un conjunto de programas que gestiona los recursos de la computadora."/>
    </preguntasC>
    <preguntasC
        Tipo="Corta"
        Texto="¿Qué es un protocolo de red?">
      <respuestas
          Texto="Un conjunto de reglas para la comunicación entre dispositivos en una red."/>
    </preguntasC>
    <preguntasC
        Tipo="Corta"
        Texto="¿Qué es la encapsulación en programación orientada a objetos?">
      <respuestas
          Texto="La ocultación de detalles internos y la exposición de solo lo necesario."/>
    </preguntasC>
    <preguntasC
        Texto="¿Qué es una dirección IP?"
        Rcorrecta="//@paginapantallas.1/@preguntasC.3/@respuestas.1">
      <respuestas
          Texto="a) Una unidad de medida de velocidad en internet"/>
      <respuestas
          Texto="b) Una etiqueta numérica que identifica un dispositivo en una red"/>
    </preguntasC>
    <preguntasC
        Tipo="Corta"
        Texto="¿Qué es un servidor web?">
      <respuestas
          Texto="Un software que sirve y procesa páginas web."/>
    </preguntasC>
    <preguntasC
        Tipo="Corta"
        Texto="¿Qué es una variable en programación?">
      <respuestas
          Texto="Un espacio en la memoria que almacena un valor."/>
    </preguntasC>
    <preguntasC
        Texto="¿Cuál es la diferencia entre TCP y UDP?"
        Rcorrecta="//@paginapantallas.1/@preguntasC.6/@respuestas.2">
      <respuestas
          Texto="a) TCP es más rápido que UDP"/>
      <respuestas
          Texto="b) UDP garantiza la entrega de los datos, mientras que TCP no"/>
      <respuestas
          Texto="c) TCP es orientado a la conexión y UDP no lo es"/>
    </preguntasC>
    <preguntasC
        Tipo="Corta"
        Texto="¿Qué es el ciclo de vida de un proceso en un sistema operativo?">
      <respuestas
          Texto="Las etapas que pasa un proceso desde su creación hasta su terminación."/>
    </preguntasC>
    <preguntasC
        Tipo="V_F"
        Texto="El protocolo HTTP es utilizado para transferir datos de manera segura entre el cliente y el servidor."
        Rcorrecta="//@paginapantallas.1/@preguntasC.9/@respuestas.1">
      <respuestas
          Texto="Verdadero"/>
      <respuestas
          Texto="Falso"/>
    </preguntasC>
    <preguntasC
        Tipo="V_F"
        Texto="La encapsulación en programación orientada a objetos permite ocultar los detalles de implementación y mostrar solo las interfaces necesarias."
        Rcorrecta="//@paginapantallas.1/@preguntasC.8/@respuestas.0">
      <respuestas
          Texto=" Verdadero"/>
      <respuestas
          Texto="Falso"/>
    </preguntasC>
